      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Svandis mining app"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "file-exists": "^5.0.1",
    "lodash": "^4.17.10",
    "platform": "^1.3.5"
  },
  "devDependencies": {
    "electron-prebuilt-compile": "2.0.7"
  }
